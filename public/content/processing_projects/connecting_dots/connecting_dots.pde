void draw() {
	background(10);
	
	for(int i = 0; i < blips.size(); i++) {
		Blip blip = blips.get(i);
		
		blip.update();
	}
	
	for(int i = 0; i < blips.size(); i++) {
		Blip blip = blips.get(i);
		
		blip.countNeighbors(blips);
		blip.drawLines();
	}
	
